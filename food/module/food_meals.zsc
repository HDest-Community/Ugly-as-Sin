mixin class UaS_BigMREPouch {
	default {
		UaS_Consumable.Calories 800;
		UaS_Consumable.Fluid 400;
		UaS_Consumable.Bulk 18;
		+UaS_Consumable.PACKAGED;
		+UaS_Consumable.RESEALABLE;
		Inventory.PickupMessage "$UAS_MIXIN_BIGMREPOUCH_PICKUP";
	}
}

class UaS_GarlicBeefStroganoffMRE : UaS_Consumable {
	mixin UaS_BigMREPouch;
	default {
		tag "$UAS_CONSUMABLE_BEEFANDGARLICSTROGANOFF_TAG";
		UaS_Consumable.Description "$UAS_CONSUMABLE_BEEFANDGARLICSTROGANOFF_DESCRIPTION";
	}
	override void Messages() {
		AddOpenText("$UAS_CONSUMABLE_BEEFANDGARLICSTROGANOFF_OPENTEXT");
		AddConsumeText("$UAS_CONSUMABLE_BEEFANDGARLICSTROGANOFF_CONSUMETEXT_1");
		AddConsumeText("$UAS_CONSUMABLE_BEEFANDGARLICSTROGANOFF_CONSUMETEXT_2");
	}
}

